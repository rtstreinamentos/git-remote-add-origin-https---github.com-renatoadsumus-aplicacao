<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="CadastroCategoria.aspx.cs"
    Inherits="OrcamentoNet.View.CadastroCategoria" ValidateRequest="false" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">

<%@ Register Assembly="FreeTextBox" Namespace="FreeTextBoxControls" TagPrefix="FTB" %>

<head runat="server">
    <title></title>
</head>
<body>
    <form id="form1" runat="server">
    <div>
        Id Categoria
        <asp:TextBox ID="uxtxtIdCategoria" runat="server" Width="54px"></asp:TextBox>
        <br />
        Categoria Pai
        <asp:TextBox ID="uxtxtCategoriaPai" runat="server" Width="54px"></asp:TextBox>
        <br />
        <br />
        <br />
        Nome Categoria
        <asp:TextBox ID="uxtxtNomeCategoria" runat="server" Width="438px"></asp:TextBox>
        <br />
        Url Fornecedores dessa categoria
        <asp:TextBox ID="uxtxtUrlCategoriaFornecedores" runat="server" Width="517px"></asp:TextBox>
        <br />
        Url
        <asp:TextBox ID="uxtxtUrlCategoria" runat="server" Width="517px"></asp:TextBox>
        <br />
        Url Title
        <asp:TextBox ID="uxtxtUrlCategoriaTitle" runat="server" Width="980px"></asp:TextBox>
        <br />
        Url Mappings
        <asp:TextBox ID="uxtxtUrlMappigns" runat="server" Width="980px"></asp:TextBox>
        <br />
        Termo
        <asp:TextBox ID="uxtxtTermo" runat="server" Width="980px"></asp:TextBox>
        <br />
        <FTB:FreeTextBox ID="FreeTextBox1" runat="server">
                </FTB:FreeTextBox>
        <br />
        <br />
        
        <asp:Button CssClass="ie6SubmitFix" ID="uxbtnSalvar" runat="server" Text="Salvar"
            OnClick="uxbtnSalvar_Click" />
        <asp:Button CssClass="ie6SubmitFix" ID="uxtbtnBuscar" runat="server" Text="Buscar"
            OnClick="uxtbtnBuscar_Click" />
    </div>
    </form>
</body>
</html>
